{
  "name": "masspayio/masspay-php-sdk",
  "version": "1.0.0",
  "description": "Client for MasspaySdk",
  "type": "library",
  "authors": [
    {
      "name": "MassPay Inc.",
      "email": "info@masspay.io",
      "homepage": "https://www.masspay.io"
    }
  ],
  "homepage": "https://github.com/masspayio/masspay-php-sdk",
  "keywords": [
    "MasspaySdk"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "php-http/client-common": "^2.3",
    "php-http/discovery": "^1.12",
    "psr/http-client": "^1.0",
    "psr/http-client-implementation": "^1.0",
    "psr/http-factory": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "php-http/curl-client": "^2.1",
    "laminas/laminas-diactoros": "^2.4",
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "MasspaySdk\\": "lib/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}